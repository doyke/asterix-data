            <items>
                <item name="ADR" type="Fixed"><len>24</len></item>
                <item name="ID" type="Fixed"><len>48</len></item>
                <item name="MHG" type="Fixed"><len>16</len></item>
                <item name="IAS" type="Fixed"><len>16</len></item>
                <item name="TAS" type="Fixed"><len>16</len></item>
