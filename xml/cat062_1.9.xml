            <items>
                <item name="MFL" type="Fixed"><len>8</len></item>
                <item name="MD1" type="Fixed"><len>8</len></item>
                <item name="MD2" type="Fixed"><len>8</len></item>
                <item name="MDA" type="Fixed"><len>8</len></item>
